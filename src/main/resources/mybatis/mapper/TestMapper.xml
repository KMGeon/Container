<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!-- 여기서 namespace는 @mapper가 있는 패키지를 적어야됨-->
<mapper namespace="com.example.test.dao.TestMapper">

    <select id="getList" resultType="com.example.test.dto.TestDTO">
        SELECT book_id,
               title,
               category,
               price,
               insert_date
        FROM book
    </select>

    <insert id="InsertBook" parameterType="com.example.test.dto.TestDTO">
        INSERT INTO book (
                          book_id,
                          title,
                    )
        VALUES (
                #{bookId},
                #{title}
             )
    </insert>

</mapper>